datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  phone String? @unique
}

model Booking {
  id               Int      @id @default(autoincrement())
  clientName       String
  clientEmail      String
  clientPhone      String
  booking_date     DateTime
  booking_time     String
  service          String
  duration         Int
  reservation_date DateTime
  createdAt        DateTime @default(now())
}